{
    "name": "j/imap",
    "description": "Imap library for php 7.4+",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^8.3@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Masood",
            "email": "ali@zenex.co.jp"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.3",
        "ext-mbstring": "*",
        "ext-imap": "*"
    },
    "autoload": {
        "classmap": [
            "lib/"
        ],
        "psr-4": {
            "J\\Imap\\": "lib/"
        }
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml --verbose --colors=always --testdox"
    }
}
